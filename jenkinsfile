pipeline{
  agent any
  stages{
     stage("Maven Build"){
       steps{
            sh "mvn clean package"
        }
    }
    stage("deploy-dev"){
       steps{
          sshagent(['deploy']) {
            sh "chmod 777 /home/ec2-user/.jenkins -R"
            sh "cp -rp /home/ec2-user/.jenkins/workspace/maven/target/*.jar  /home/sudo/output"
            sh "scp -rp /home/sudo/output/*.jar sudo@35.173.126.85:/home/sudo"
          
        }
      }
     }
   }
}
