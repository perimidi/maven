pipeline { 
     agent any 
         stages { 
             stage ("maven build "){ 
               steps { 
                      sh "mvn clean package"    
                     }   
               }
             stage( " deploy "){ 
                steps { 
                    
                    sshagent(["deploy"[){
                       sh "chmod 777 /home/ec2-user/.jenkins -R"
                       sh "cp -rp /home/ec2-user/.jenkins/workspace/demopipeline/target/*.jar /home/test/output"
                       sh "scp -rp /home/test/output/*.jar test@34.226.155.152:/home/test"
 
                     }
                 }

             }

         }
  }
